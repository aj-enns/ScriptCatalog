{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "PowerShell",
      "request": "launch",
      "name": "Debug PowerShell Script",
      "script": "${workspaceFolder}/${input:scriptPath}",
      "args": [
        "-OrganizationName", "devops-halo"
      ],
    }
  ],
  "inputs": [
    {
      "id": "scriptPath",
      "type": "pickString",
      "description": "Select the PowerShell script to debug",
      "options": [
        "ADO/FindingCollectionContributors.ps1",
        "GitHub/FindEnterpriseContributors.ps1"
      ]
    }
  ]
}